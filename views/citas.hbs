<table border="solid">
    <h2>LISTADO DE CITAS</h2>
    <thead>
        <tr>
            <th>Fecha</th>
            <th>paciente</th>
            <th>Medico</th>
            <th>Consultorio</th>
        </tr>
    </thead>
    <tbody>
       {{#each citas}}
       <tr>
        <td>{{fecha}}</td>
        <td>{{nombre}}</td>
        <td>{{nombres}}</td>
        <td>{{consultorio}}</td>
        <td>
                <a class="eliminar" href="/citas/eliminar/{{id}}">Eliminar</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
<div class="opciones-tabla">
    <a class="volver" href="/">Volver</a>
    <a href="/citas/agregar-cita">Nueva cita</a>
</div>
